stage 'CI'
node {
checkout scm
    stage('checkout') {
        git 'https://github.com/memory1/HelloWorld.git'
     }
    stage('msbuild') {
        bat label: '', script: 'msbuild HelloWorld.sln'
    }
    stage('archive') {
                    archiveArtifacts 'HelloWorld\\bin\\Debug\\*.*'
         }
}
