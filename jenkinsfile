stage('CI')
node('master') {
    stage('checkout') {
        git 'https://github.com/memory1/HelloWorld.git'
     }
    stage('msbuild') {
        bat label: '', script: 'msbuild HelloWorld.sln'
    }
   stage('mstest') {
        bat label: '', script: 'mstest /testcontainer:.\\HelloWorldTests\\bin\\Debug\\HelloWorldTests.dll'
    }
    stage('archive') {
                    archiveArtifacts 'HelloWorld\\bin\\Debug\\*.*'
         }
}
